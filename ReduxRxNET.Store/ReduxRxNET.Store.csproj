<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <PackageId>ReduxRxNET.Store</PackageId>
    <PackageVersion>0.0.1-alpha</PackageVersion>
    <Authors>Lander Verhack</Authors>
    <Description>.NET implementation for the Redux pattern, meant for Reactive Programming</Description>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/LanderVe/ReduxRxNET.Store/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/LanderVe/ReduxRxNET.Store</PackageProjectUrl>
    <PackageReleaseNotes>First alpha</PackageReleaseNotes>
    <Copyright>Copyright 2017 Lander Verhack. All rights reserved.</Copyright>
    <PackageTags>ReduxRxNET Redux Rx Reactive Store</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>ReduxRxNET.Store.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Reactive" Version="3.1.1" />
  </ItemGroup>

</Project>